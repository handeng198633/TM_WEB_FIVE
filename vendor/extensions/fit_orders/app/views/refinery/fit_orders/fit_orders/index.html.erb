<% content_for :body do %>
  <ul id="fit_orders">
    <% @fit_orders.each do |fit_order| %>
      <li>
        <%= link_to fit_order.group_number, refinery.fit_orders_fit_order_path(fit_order) %>
      </li>
    <% end %>
  </ul>



<table id="fit_orders" class="display" data-source="<%= fit_orders_url(format: "json") %>">
  <thead>
    <tr>
      <th>订单编号</th>
      <th>团号</th>
      <th>出发日期</th>
      <th>地接日期</th>
      <th>订购方</th>
      <th>出团通知</th>
      <th>人数[名单]</th>
      <th>接</th>
      <th>送</th>
      <th>收客价/地接价</th>
      <th>西藏结算价/当地成本</th>
      <th>客人信息</th>
      <th>备注</th>
      <th>接受人</th>
      <th>当前已收款</th>
      <th>状态</th>
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>


<% end %>

<%= render '/refinery/content_page' %>


